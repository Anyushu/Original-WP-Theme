{
  "name": "anyushu",
  "version": "1.0.0",
  "description": "Original WP Theme",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-sass": "node-sass --include-path scss assets/scss/style.scss lib/css/style.css",
    "build": "node-sass --include-path scss assets/scss/style.scss lib/css/style.min.css --output-style compressed",
    "watch": "node-sass --include-path scss assets/scss/style.scss lib/css/style.css -w",
    "imgmin": "node assets/js/imagemin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anyushu/Original-WP-Theme.git"
  },
  "author": "anyushu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Anyushu/Original-WP-Theme/issues"
  },
  "homepage": "https://github.com/Anyushu/Original-WP-Theme#readme",
  "dependencies": {
    "node-sass": "^4.13.0"
  },
  "devDependencies": {
    "imagemin": "^7.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0"
  }
}
